<div id="<%= dom_id movie %>">
  <p>
    <strong>Title:</strong>
    <%= movie.title %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= movie.image_url %>
  </p>

  <p>
    <strong>Trailer url:</strong>
    <%= movie.trailer_url %>
  </p>

  <p>
    <strong>Movie url:</strong>
    <%= movie.movie_url %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= movie.status %>
  </p>

  <p>
    <strong>Release date:</strong>
    <%= movie.release_date %>
  </p>

  <p>
    <strong>Watched date:</strong>
    <%= movie.watched_date %>
  </p>

</div>
